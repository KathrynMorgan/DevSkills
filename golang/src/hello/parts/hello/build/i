// Greet World
func greet() {
	sum := 0
	g := "wElL hElLo mY CrAzEe wErLd!"
	for i := 0; i < 3; i++ {
		fmt.Printf("%s\n", g)
		fmt.Printf("%s\n", time.Date())
		sum = sum + i
	}
}

// Core Logic
func main() {
	greet()
}
